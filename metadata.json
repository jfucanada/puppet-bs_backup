{
  "name": "alameow-bs_backup",
  "version": "0.1.0",
  "author": "alameow",
  "summary": "A puppet module which manage file backup, most useful when your config files are managed by inifile or augeas and so on.",
  "license": "Apache 2.0",
  "source": "git@github.com:bloodhero/puppet-bs_backup.git",
  "issues_url": "https://github.com/bloodhero/puppet-bs_backup/issues",
  "project_page": "https://github.com/bloodhero/puppet-bs_backup",
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.0.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0"
    }
  ],
  "dependencies": [
    {"version_requirement":">= 3.2.2","name":"puppetlabs-stdlib"}
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [ "5", "6", "7" ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [ "5", "6", "7" ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [ "5", "6", "7" ]
    },
    {
      "operatingsystem": "OEL",
      "operatingsystemrelease": [ "5", "6", "7" ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [ "5", "6", "7" ]
    }
  ]
}
